class MileageLog < ActiveRecord::Base

    belongs_to :student
    belongs_to :week

    attr_accessible(:miles_monday)
    attr_accessible(:miles_wednesday)
    attr_accessible(:miles_rest_of_week)
    attr_accessible(:attendance_monday)
    attr_accessible(:attendance_wednesday)
    attr_accessible(:student_id)
    attr_accessible(:week_id)

end
